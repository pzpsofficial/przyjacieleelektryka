*::after {\n  margin: 0;\n  padding: 0;\n  box-sizing: inherit; }\n\n::selection {\n  color: #f1f1e8;\n  background-color: #81b214; }\n\nhtml {\n  scroll-behavior: smooth;\n  font-size: 62.5%; }\n\nbody {\n  background-color: #f1f1e8;\n  box-sizing: border-box;\n  font-family: 'Roboto', sans-serif;\n  display: grid;\n  grid-template-columns: repeat(12, 1fr); }\n\n.text--thin {\n  font-weight: 400; }\n\n.text--bold {\n  font-weight: 500; }\n\n.text--very--bold {\n  font-weight: 700; }\n\n.text--light {\n  color: #f1f1e8; }\n\n.text--green {\n  color: #81b214; }\n\n.text--dark {\n  color: #131313; }\n\n.text--border {\n  border-bottom: 4px solid #81b214; }\n\n.text--italic {\n  font-style: italic;\n  font-size: 2.6rem;\n  border-left: 4px solid #81b214;\n  padding-left: 2rem; }\n\nh1 {\n  font-size: 6rem; }\n\nh2 {\n  text-transform: uppercase;\n  text-align: center;\n  font-size: 5rem; }\n\nh3 {\n  font-size: 4rem; }\n\nh4 {\n  font-size: 3rem; }\n\np {\n  font-size: 2.3rem;\n  color: #131313; }\n\n.mb-large {\n  margin-bottom: 5rem; }\n\n.mb-big {\n  margin-bottom: 3rem; }\n\n.mb-medium {\n  margin-bottom: 2rem; }\n\n.mb-small {\n  margin-bottom: 1rem; }\n\n.mt-large {\n  margin-top: 5rem; }\n\n.mt-big {\n  margin-top: 3rem; }\n\n.mt-medium {\n  margin-top: 2rem; }\n\n.mt-small {\n  margin-top: 1rem; }\n\n.btn {\n  display: inline-block;\n  background-color: #81b214;\n  border-radius: 5px;\n  padding: 1.5rem 4rem;\n  font-size: 3.5rem;\n  font-family: inherit;\n  text-decoration: none;\n  color: #f1f1e8;\n  position: relative;\n  backface-visibility: hidden;\n  z-index: 1;\n  overflow: hidden;\n  transition: all 0.3s ease-in; }\n  .btn::before {\n    height: 100%;\n    content: '';\n    z-index: -1;\n    position: absolute;\n    width: 100%;\n    background-color: rgba(32, 106, 93, 0.5);\n    transition: transform 1s cubic-bezier(0.68, -0.6, 0.32, 1.6);\n    top: 0;\n    left: 0;\n    transform: translateX(110%) scale(1.1);\n    border-radius: 5px;\n    backface-visibility: hidden; }\n  .btn:hover {\n    color: #f1f1e8 !important; }\n  .btn:hover::before {\n    transform: translateX(0) scale(1.1); }\n\n.bg-video {\n  position: absolute;\n  top: 0;\n  left: 0;\n  height: 100%;\n  width: 100%;\n  z-index: -1;\n  overflow: hidden; }\n  .bg-video__content {\n    height: 100%;\n    width: 100%;\n    object-fit: cover; }\n\n.main {\n  grid-column: 1/-1; }\n\n.header {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  min-height: 7vh;\n  z-index: 100;\n  display: flex;\n  justify-content: space-between;\n  align-items: center; }\n  .header__burger {\n    display: none; }\n  .header__branding {\n    margin-left: 3%;\n    display: flex;\n    justify-content: space-between;\n    align-items: center; }\n    .header__branding i {\n      margin-right: 2rem;\n      color: #81b214;\n      font-size: 3rem; }\n  .header__nav {\n    margin-right: 3%; }\n    .header__nav__ul {\n      list-style: none;\n      display: flex;\n      justify-content: space-between;\n      align-items: center; }\n    .header__nav__li:not(:last-child) {\n      margin-right: 2.5rem; }\n    .header__nav__a {\n      color: #f1f1e8;\n      text-decoration: none;\n      font-size: 2rem;\n      text-transform: uppercase;\n      transition: color 0.3s ease-in; }\n      .header__nav__a:hover {\n        color: #81b214; }\n\n.section-hero {\n  padding-top: 7vh;\n  grid-column: 1/-1;\n  min-height: 100vh;\n  position: relative;\n  background-image: linear-gradient(rgba(19, 19, 19, 0.75), rgba(19, 19, 19, 0.75));\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center; }\n\n.section-about {\n  grid-column: 2/-2; }\n  .section-about__row {\n    width: 100%;\n    display: grid;\n    grid-gap: 2rem;\n    align-items: center;\n    justify-items: stretch;\n    grid-template-columns: repeat(2, 50%); }\n    .section-about__row__content {\n      padding: 2rem;\n      display: flex;\n      flex-direction: column;\n      justify-content: center;\n      align-items: flex-start; }\n    .section-about__row__img {\n      padding: 2rem;\n      position: relative;\n      overflow: hidden; }\n      .section-about__row__img img {\n        border-radius: 5px;\n        box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.2);\n        display: block;\n        width: 100%;\n        margin: auto; }\n\n.section-projects {\n  grid-column: 2/-2;\n  display: grid;\n  grid-template-columns: 1fr; }\n  .section-projects__buttons {\n    display: grid;\n    justify-content: center;\n    align-content: center;\n    grid-gap: 2rem;\n    justify-items: center;\n    grid-template-columns: repeat(auto-fit, minmax(min-content, 30rem)); }\n    .section-projects__buttons__button {\n      display: inline-block;\n      padding: 3rem 7rem;\n      border-radius: 5px;\n      background-color: #131313;\n      color: #f1f1e8; }\n\n.active {\n  background-color: #81b214; }\n\n.slider {\n  width: 100%;\n  box-shadow: 0 1rem 2rem rgba(0, 0, 0, 0.25);\n  background-color: #fff;\n  position: relative;\n  padding: 0 3rem;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  text-align: center; }\n\n.arrow-left,\n.arrow-right {\n  position: absolute;\n  top: 50%;\n  font-size: 5rem;\n  opacity: 0.5;\n  transition: opacity 0.5s ease-in-out; }\n  .arrow-left:hover,\n  .arrow-right:hover {\n    cursor: pointer;\n    opacity: 0.8; }\n\n.arrow-left {\n  transform: translate(-50%, -50%);\n  left: 0; }\n\n.arrow-right {\n  transform: translate(50%, -50%);\n  right: 0; }\n\n.dot {\n  width: 20px;\n  height: 20px;\n  background-color: rgba(19, 19, 19, 0.5);\n  border-radius: 50%;\n  transition: background-color 0.3s ease-in; }\n\n.active {\n  background-color: rgba(19, 19, 19, 0.8); }\n\n.imgs {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-evenly; }\n  .imgs img {\n    max-width: 300px;\n    box-shadow: 0 0.25rem 0.5rem rgba(0, 0, 0, 0.15);\n    border-radius: 5px; }\n\n.indicators {\n  display: flex;\n  justify-content: center;\n  align-items: center; }\n  .indicators .dot:not(:last-child) {\n    margin-right: 1.3rem; }\n"},"lineCount":294}},"error":null,"hash":"bcb9ff27e76399aed0ba618a83e2c56c","cacheData":{"env":{}}}